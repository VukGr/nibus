{
   "trailingComma": "es5",
   "tabWidth": 2,
   "semi": false,
   "singleQuote": true,
   "jsxSingleQuote": true,
   "bracketSpacing": true,
   "printWidth": 80,
   "endOfLine": "auto",
   "tailwindPreserveWhitespace": true,
   "endingPosition": "absolute-with-indent",
   "plugins": [
      "prettier-plugin-tailwindcss",
      "prettier-plugin-classnames",
      "prettier-plugin-merge"
   ]
}
